
{
  "version": 2,
  "builds": [
    {
      "src": "Trader/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Trader/wsgi.py"
    }
  ],
  "crons": [
    {
      "path": "/Worker/worker.py",
      "schedule": "* * * * *"
    }
  ],
  "outputDirectory": "static"
}